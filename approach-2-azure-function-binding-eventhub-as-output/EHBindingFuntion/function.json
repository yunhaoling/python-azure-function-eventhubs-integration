{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "eventHub",
      "name": "$return",
      "eventHubName": "<eh_name>",
      "connection": "EHConnectionString",
      "direction": "out"
    }
  ]
}
